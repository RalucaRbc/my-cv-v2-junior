.column(span="row")
  .section__title-wrapper
    .section__title=sectionTitle
.column(span="row")
  input.youtube__checkbox(type="checkbox", id="portrait")
  label(for="portrait").disc__red
    .icon__youtube
  .portrait
    .close
      label(for="portrait").disc__green
        i.icon__cross
    .portrait__body
      a.youtube__video(href="https://www.youtube.com/watch?v=0VvTBwJQMoQ&t=15s" target="_blank")
        img(src="asset/image/youtube.png", alt="")
